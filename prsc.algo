PROCEDURE tri(var tab: ARRAY_OF INTEGER)
VAR
    i,j,k:INTEGER
BEGIN
FOR i FROM 0 TO tab.length  DO
   k:=t[i];
   j:=i;
   WHILE (j>=0 AND tab[j-1]>k) DO
   tab[j]:=tab[j-1]
   j:=j-1
   END_WHILE
   tab[j]=k
END_FOR
 
END
ALGORITHM  insertion_tri
VAR
 t : ARRAY_OF INTEGER[length];
BEGIN
    tri(t);
END
